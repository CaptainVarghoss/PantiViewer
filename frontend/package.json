{
  "name": "pantiviewer",
  "private": true,
  "version": "0.0.1",
  "main": "electron.cjs",
  "type": "module",
  "scripts": {
    "start-react": "vite",
    "start-fastapi": "uvicorn --app-dir ../backend main:app --reload --host 0.0.0.0 --port 8000",
    "start-electron": "wait-on http://localhost:5173 && electron .",
    "dev": "concurrently \"npm run start-react\" \"npm run start-fastapi\" \"npm run start-electron\"",
    "headless-dev": "concurrently \"npm run start-react\" \"npm run start-fastapi\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "framer-motion": "^12.23.24",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "react-select": "^5.10.1",
    "react-window": "^2.2.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "wait-on": "^9.0.3"
  }
}
